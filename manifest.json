{
    "manifest_version": 2 ,
    "name":"Netone Cards",
    "version" : "1",
    "content_scripts": [
        {
         "matches":[
             "<all_urls>"
         ],
         "js": ["content.js"]
        }
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
     },
    "permissions":["tabs"],
    "browser_action": {
        "default_popup":"popup.html",
        "default_title" : "Print Cards"
    }
}